<?xml version='1.0' encoding='utf-8'?>
<widget id="com.opratel.opraTest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SmsLog">
        <param name="android-package" value="be.creacore.smslog.SmsLog" />
    </feature>
    <feature name="Screenshot">
        <param name="android-package" value="com.darktalker.cordova.screenshot.Screenshot" />
    </feature>
    <name>opraTest</name>
    <description>
        Aplicacion para el uso de la compa√±ia Opratel 
    </description>
    <author email="ramiro.portas@gmail.com" href="http://cordova.io">
        ramiro ariel portas
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="#212121" />
    <preference name="Orientation" value="portrait" />
</widget>
